
class two_digit_sub
{
	public static void main(String[] args) 
	{
		int a =343418,count=0,value;
		int len =(a+" ").length();
		
		for (int i = 0;i<=len -1;i++){
			for(int j = i+1;j<=len-1;j++){
				if (i==j){
					count++;
					value = i;
				}
				else{
					count = 0;
					value=0;
				}
				if (value>i+1)
					{
					value = i;
				}
				else{
					value = i+1;
				}
				System.out.println(count);
				System.out.println(value);
					
			}}
		}
}